{
  "name": "brisky-render",
  "version": "2.0.19-f91a008231eb2d552d9128729c4d2eccba17c28c",
  "description": "A js library for creating blazing fast ui's",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "browser": {
    "./dist/index.js": "./dist/browser.js",
    "./dist/index.es.js": "./dist/browser.es.js",
    "./src/property/style/sheet/render": "./src/property/style/sheet/render/browser",
    "./src/render/dom/create/render": "./src/render/dom/create/render/browser",
    "./src/events/listener": "./src/events/listener/browser",
    "./src/render/merge": "./src/render/merge/browser",
    "html-element/global-shim": false
  },
  "scripts": {
    "build": "node rollup/build",
    "dev": "node rollup/watch & nodemon --harmony-async-await test/index.js",
    "dev-browser": "node rollup/watch & budo test/index.js --live --- -g bubleify",
    "pretest": "npm run build && standard",
    "test": "node test/index.js",
    "version-commit": "npm --no-git-tag-version version $(node -pe \"require('./package.json').version.split('-')[0]\")-$(git log -n 1 --pretty=format:'%H')",
    "publish-unstable": "npm run version-commit && npm publish --tag unstable",
    "prepublish": "npm run build",
    "cover": "nyc npm test",
    "travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-render.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-render/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-render#readme",
  "dependencies": {
    "brisky-stamp": "^4.0.0",
    "brisky-struct": "git://github.com/vigour-io/brisky-struct.git#feature/enhancements",
    "html-element": "^2.1.0",
    "string-hash": "^1.1.0",
    "vigour-ua": "^3.0.0"
  },
  "devDependencies": {
    "budo": "^9.3.0",
    "bubleify": "0.5.1",
    "coveralls": "^2.11.12",
    "nodemon": "^1.10.2",
    "nyc": "^10.0.0",
    "parse-element": "^2.0.5",
    "pre-commit": "^1.1.3",
    "rollup": "^0.37.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^2.5.0",
    "source-map-support": "^0.4.7",
    "standard": "^8.0.0",
    "strip-formatting": "^1.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "keywords": [
    "reactive",
    "state",
    "element",
    "DOM"
  ],
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io>"
  ]
}
