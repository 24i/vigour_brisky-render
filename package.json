{
  "name": "brisky-core",
  "version": "1.6.9",
  "description": "The core of Brisky, a js library for creating blazing fast web applications.",
  "main": "lib/element.js",
  "scripts": {
    "test": "(ducktape; node test) | tap-difflet && standard",
    "watch": "nodemon test | tap-difflet",
    "watch-browser": "budo ./test/index.js -p 8080 --live",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "docs": "node_modules/vigour-doc/bin/vdoc",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-core.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-core/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-core#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "html-element": "^2.1.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "vigour-observable": "^3.1.18",
    "vigour-stamp": "^1.4.0",
    "vigour-state": "^4.4.6",
    "vigour-ua": "^2.1.0",
    "vigour-util": "^3.3.2"
  },
  "devDependencies": {
    "ducktape": "^1.0.1",
    "budo": "^9.0.0",
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.5",
    "nodemon": "^1.10.2",
    "pre-commit": "^1.1.3",
    "standard": "^8.0.0",
    "tap-difflet": "0.6.0",
    "tape": "^4.6.0",
    "vigour-doc": "^1.1.6",
    "parse-element": "^2.0.4"
  },
  "browser": {
    "./lib/render/dom/create/render": "./lib/render/dom/create/render/browser",
    "html-element": false
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "keywords": [
    "reactive",
    "state",
    "element",
    "DOM"
  ],
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io>"
  ]
}
