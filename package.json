{
  "name": "brisky-render",
  "version": "0.0.1",
  "description": "The core of Brisky, a js library for creating blazing fast web applications.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "browser": {
    "./dist/index.js": "./dist/browser.js",
    "./dist/index.es.js": "./dist/browser.es.js",
    "./lib/render/dom/create/render": "./lib/render/dom/create/render/browser",
    "./lib/events/listener": "./lib/events/listener/browser",
    "html-element/global-shim": false
  },
  "scripts": {
    "build": "node rollup/build",
    "dev": "node rollup/watch",
    "postinstall": "[ -d dist ] || npm run build || ( mkdir dist && yarn && npm run build )",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-core.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-core/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-core#readme",
  "dependencies": {
    "brisky-stamp": "^3.0.10",
    "brisky-struct": "git+ssh://git@github.com/vigour-io/brisky-struct#feature/rollup",
    "html-element": "^2.1.0",
    "styletron": "^2.2.0",
    "styletron-utils": "^2.2.1",
    "quick-hash": "^1.0.1",
    "vigour-ua": "^2.2.3"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.5",
    "nodemon": "^1.10.2",
    "parse-element": "^2.0.5",
    "pre-commit": "^1.1.3",
    "rollup": "^0.37.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^2.5.0",
    "standard": "^8.0.0",
    "strip-formatting": "^1.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "standard": {
    "ignore": [
      "/wip/"
    ]
  },
  "keywords": [
    "reactive",
    "state",
    "element",
    "DOM"
  ],
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io>"
  ]
}
