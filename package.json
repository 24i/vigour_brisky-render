{
  "name": "brisky-render",
  "version": "2.0.10",
  "description": "A js library for creating blazing fast ui's",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "browser": {
    "./dist/index.js": "./dist/browser.js",
    "./dist/index.es.js": "./dist/browser.es.js",
    "./lib/property/style/sheet": "./lib/property/style/sheet/browser",
    "./lib/render/dom/create/render": "./lib/render/dom/create/render/browser",
    "./lib/events/listener": "./lib/events/listener/browser",
    "html-element/global-shim": false
  },
  "scripts": {
    "build": "node rollup/build",
    "dev": "node rollup/watch & nodemon --harmony-async-await test/index.js",
    "dev-browser": "node rollup/watch & budo test/index.js --live --- -g bubleify",
    "pretest": "npm run build && standard",
    "prepublish": "npm run build",
    "test": "node test/index.js",
    "postinstall": "[ -d dist ] || npm run build || ( mkdir dist && yarn && npm run build )",
    "cover": "nyc npm test",
    "travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-render.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-render/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-render#readme",
  "dependencies": {
    "brisky-stamp": "^3.0.11",
    "brisky-struct": "^1.0.7",
    "html-element": "^2.1.0",
    "string-hash": "^1.1.0",
    "vigour-ua": "^2.2.3"
  },
  "devDependencies": {
    "budo": "^9.3.0",
    "bubleify": "0.5.1",
    "coveralls": "^2.11.12",
    "nodemon": "^1.10.2",
    "nyc": "^10.0.0",
    "parse-element": "^2.0.5",
    "pre-commit": "^1.1.3",
    "rollup": "^0.37.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^2.5.0",
    "source-map-support": "^0.4.7",
    "standard": "^8.0.0",
    "strip-formatting": "^1.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "keywords": [
    "reactive",
    "state",
    "element",
    "DOM"
  ],
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io>"
  ]
}
